<div class="book_container">
  <div class="book_card">
    <div class="row">
      <div class="col">
        <img src="<%= element.link %>" class="book_pic">
      </div>
      <div class="col">
        <h1><%= element.title %></h1>
        <p><b>Autor: <%= element.author %></b></p>
        <p><%= element.description %></p>
      </div>
    </div>
  </div>
</div>
